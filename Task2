#include<stdio.h>

int main(){
    int n = 0;
    int sum = 0;
    printf("input number:");
    scanf("%d",&n);
    for(int i = 1; i <= n / 2;i++){
        if(n % i == 0){
            sum += i;
        }
    }
    if(sum == n){
        printf("number is perfect:\n");
    }else{
        printf("number is not perfect:\n");
    }
    return 0;
}
